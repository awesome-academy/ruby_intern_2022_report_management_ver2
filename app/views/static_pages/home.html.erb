<% provide :title, t(".title") %>
<% if user_signed_in? %>
    <h1 id="department-report"><%= t ".department_report" %></h1>
    <%= render "shared/feed" %>
<% else %>
  <div class = "mgr-25">
    <h1><%= t ".title" %></h1>
    <p><%= t ".home_content" %></p>
    <div class="center jumbotron">
      <%= link_to t(".login_now"), new_user_session_url, class: "btn btn-lg btn-primary" %>
    </div>
  </div>
<% end %>
<%= render "shared/modal" %>
