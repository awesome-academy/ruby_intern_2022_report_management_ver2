<li id="report-<%= report.id %>">
  <table class="report-table">
    <tr>
      <th></th>
      <th class="user"><span><%= t ".name" %></span></th>
      <th class="plan_today"><span><%= t ".plan_today" %></span></th>
      <th class="time"></th>
      <th><span><%= t ".status" %></span></th>
      <th class="edit"></th>
    </tr>
    <tr>
      <td><%= link_to gravatar_for(report.user, size: Settings.digit_50), report.user %></td>
      <td class="user"><span><%= link_to report.user_name, report %></span></td>
      <td class="plan_today"><span><%= report.plan_today %></span></td>
      <td class="time"><%= l(report.created_at, format: :short) %></td>
      <td class="status color-<%= report.status%>"><span ><%= report.status %></span></td>
      <td class="edit">
        <span><%= link_to t(".edit"), edit_report_url(id: report.id) %></span>
      </td>
    </tr>
  <table>
</li>
